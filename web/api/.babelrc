{
  "presets": [
    "latest", [
      "env",
      {
        "targets": {
          "node": "10"
        }
      }
    ]
  ],
  "plugins": [
    ["transform-runtime",
	{
	  "polyfill": false,
	  "regenerator": true,
	}],
    ["module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "controllers": "./src/controllers",
          "models": "./src/models",
          "utils": "./src/libs/utils"
        }
      }]
  ]
}
