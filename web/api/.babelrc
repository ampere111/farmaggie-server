{
  "presets": [
    "latest", [
      "env",
      {
        "targets": {
          "node": "8.12.0"
        }
      }
    ]
  ],
  "plugins": [
    ["transform-runtime"],
    ["module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "controllers": "./src/controllers",
          "models": "./src/models",
          "utils": "./src/libs/utils"
        }
      }]
  ]
}